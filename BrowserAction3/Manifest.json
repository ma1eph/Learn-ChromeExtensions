{
  "name": "BrowserAction3",
  "version": "1.0",

  "description": "Sample browser_action",

  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],

  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },

  "browser_action": {
    "default_title": "BrowserAction3",
    "default_icon": "icon19.png"
  },

  "manifest_version": 2
}
