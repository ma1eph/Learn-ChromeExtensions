{
  "name": "KeyBind2",
  "description": "Sample content_scripts",
  "version": "1.0",

  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],

  "background": {
    "scripts": [ "background.js" ]
  },

  "content_scripts": [{
    "matches": [ "*://*/*" ],
    "js": [ "content.js" ]
  }],

  "manifest_version": 2
}
